	<%!//선언문

	

	public void m() {
		System.out.println("mmm");
	}%>

	<%
	//서비스 메소드로 해석됨. Scriptlet
	
	int k = 10;
	
	for (int i = 0; i < 5; i++) {
		
		out.println(i + " : for문입니다.");
	}
	
	out.println("그냥 입출력입니다.");
	m();
	k++;
	%>
	<h1>k값을 출력</h1>
	<%=k%>

